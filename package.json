{
  "name": "hardhat-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy": "hardhat --network bsc_testnet deploy",
    "verify": "hardhat --network bsc_testnet etherscan-verify --solc-input",
    "export": "hardhat --network bsc_testnet export --export deployments/avalanche.json",
    "deploy-bsc-testnet": "npx hardhat run --network bsc_testnet scripts/1_initialize_grn.ts",
    "deploy-bsc-mainnet": "npx hardhat run --network bsc_mainnet scripts/1_initialize_grn.ts",
    "prepare": "husky install",
    "format": "prettier --write \"tests/**/*.ts\" \"tasks/**/*.ts\" \"contracts/**/*.sol\" \"deploy/**/*.ts\" \"hardhat.config.ts\"",
    "pretty-quick": "pretty-quick --staged",
    "coverage": "npm run build && npx hardhat coverage --temp artifacts --network coverage"
  },
  "devDependencies": {
    "@0x/subproviders": "^6.6.5",
    "@ethersproject/abstract-provider": "^5.6.1",
    "@ethersproject/constants": "^5.6.1",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.6",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.8",
    "ts-node": "^10.8.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.2",
    "web3-provider-engine": "^16.0.4"
  }
}
